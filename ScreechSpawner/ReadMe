Spawns a Screech That is Customizable!

ExampleScreech = {
    Model="https://github.com/MuhXd/DoorSuff/blob/main/ScreechSpawner/ScreechDefault.rbxm?raw=true",
    Hurts = true,
}

-----[[ Advanced Settings ]]-----
entity.Debug.Spawned = function(NameofModel,Configs)
    print("Entity Has Spawned : Model Name", NameofModel .. " Settings Are"..Configs)
end

entity.Debug.Looked = function(NameofModel)
    print("Entity has Been Looked At : Model Name".. NameofModel)
end
entity.Debug.TimeOut = function(NameofModel)
    print("Entity has Not Been Looked At In Time : Model Name".. NameofModel)
end

entity.Debug.Despawned = function(NameofModel)
    print("Entity Has Despawned : Model Name".. NameofModel)
end





